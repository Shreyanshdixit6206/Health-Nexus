{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/health-nexus.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/frontend/health-nexus.html"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/health-nexus.html"
    }
  ]
}
